{
  "name": "pantry-project-frontend",
  "version": "2.0.0",
  "description": "Pantry - Frontend",
  "author": "Pantry",
  "license": "UNLICENSED",
  "repository": "none",
  "proxy": "http://localhost:3010",
  "dependencies": {
    "@emotion/react": "*",
    "@emotion/styled": "*",
    "@material-ui/core": "*",
    "@material-ui/icons": "*",
    "@mui/icons-material": "*",
    "@mui/material": "*",
    "@react-three/drei": "^9.43.3",
    "@react-three/fiber": "^8.9.1",
    "emoji-picker-react": "*",
    "prop-types": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "6.4.2",
    "react-router-dom": "6.4.2",
    "styled-components": "^5.3.6",
    "three": "^0.146.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "bestzip": "*",
    "eslint-config-google": "*",
    "eslint-plugin-react": "*",
    "eslint-plugin-react-hooks": "*",
    "msw": "*",
    "puppeteer": "*",
    "puppeteer-core": "*",
    "react-scripts": "*"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts start",
    "test": "react-scripts test --coverage",
    "lint": "eslint ./src/**/*.js"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js"
    ],
    "testMatch": [
      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "google"
    ],
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "indent": [
        "warn",
        2
      ]
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
